<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.classAttendance.models.pojo.TbStudentEntity" table="tb_student" schema="class_attendance">
        <id name="id" column="id"/>
        <property name="studentCode" column="student_code"/>
        <property name="studentName" column="student_name"/>
        <property name="studentSex" column="student_sex"/>
        <property name="studentPhone" column="student_phone"/>
        <property name="studentEmail" column="student_email"/>
        <property name="courseSet" column="course_set"/>
    </class>
</hibernate-mapping>